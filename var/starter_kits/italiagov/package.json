{
  "name": "bootstrap_italia",
  "version": "1.4.3",
  "description": "'A Drupal theme which implements the Italia Design System'",
  "main": "src/js/index.js",
  "engineStrict": true,
  "engines": {
    "node": ">=v12.0.0"
  },
  "scripts": {
    "test": "npm run test-htmlprofiler",
    "build:dev": "cross-env NODE_ENV=development webpack --progress --config ./webpack.dev.js",
    "build:prod": "cross-env NODE_ENV=production webpack --progress --config ./webpack.prod.js",
    "watch:dev": "cross-env NODE_ENV=development webpack --watch --progress --config ./webpack.dev.js",
    "watch:prod": "cross-env NODE_ENV=production webpack --watch  --progress --config ./webpack.prod.js"
  },
  "repository": {
    "type": "git",
    "url": "git@git.drupal.org:project/bootstrap_italia.git"
  },
  "keywords": [
    "css",
    "sass",
    "mobile-first",
    "responsive",
    "front-end",
    "framework",
    "web"
